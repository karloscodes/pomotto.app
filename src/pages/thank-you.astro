---
import Layout from "../layouts/Layout.astro";
import { pages } from "../config/seo.ts";

const { title, description, url, image } = pages.thankYou;
---

<Layout
  title={title}
  description={description}
  url={url}
  image={image}
>
  <section class="mt-32 mb-10 bg-white">
    <div class="container mx-auto w-8/12">
      <div class="mt-10 text-justify">
        <h1 class="text-4xl font-bold">Success!</h1>
        <p class="mt-4">
          Thank you for reaching out to us. We will get back to you as soon as
          possible.
        </p>
      </div>
    </div>
  </section></Layout
>
